<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Vikings</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName = "VFEV_PlantGrowerArtifact"]/label</xpath>
				<value>
					<label>prothean cornucopia</label>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName = "VFEV_PlantGrowerArtifact"]/description</xpath>
				<value>
					<description>A mysterious, single-use artifact which grows all plants nearby to maturity when activated. Created by a superior AI, it served as a gift to humanity long ago.</description>
				</value>
			</li>
			</operations>
		</match>
	</Operation>
</Patch>